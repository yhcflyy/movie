<view class="page"><swiper class="swiperStyle" autoplay="true" indicator-dots="true" circular="true" indicator-active-color="white" indicator-color="grey"><block wx:for="{{bannerItems}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><swiper-item><image class="swiper-image" src="{{item.img}}" mode="aspectFill" webp="true" lazy-load="{{true}}"></image><view class="swiper_bottom"><text class="swiper-item-text">{{item.title}}</text></view></swiper-item></block></swiper><block wx:for="{{recommendItems}}" wx:for-item="item" wx:for-index="__i1__" wx:key="albumId"><view class="album"><view class="album-top">{{item.albumTitle}}</view><view class="album-container"><block wx:for="{{item.videoList}}" wx:for-item="video" wx:for-index="__i2__" wx:key="videoId"><view class="album-item"><image class="swiper-image" src="{{video.imgUrl}}" mode="{{video.imageMode}}" webp="true" lazy-load="{{true}}" data-event-opts="{{[['error',[['imageError',['$0'],[[['recommendItems','albumId',item.albumId],['videoList','videoId',video.videoId]]]]]]]}}" binderror="__e"></image><view class="video-bottom"><text class="video-bottom-text">{{video.title}}</text></view></view></block></view></view></block></view>